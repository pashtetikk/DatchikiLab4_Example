<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ScottPlot.WinForms" Version="5.0.47" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="ISignalGenerator">
      <HintPath>ISignalGenerator.dll</HintPath>
    </Reference>
    <Reference Include="SignalGenLib">
      <HintPath>bin\Debug\net6.0-windows\SignalGenLib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>